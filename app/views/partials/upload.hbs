{{#if isAuthenticated}}
	<form action="/" method="post" enctype="multipart/form-data">
		<p>Name : <br> <input type=text name=name></input></p>
		<p>Info : <br> <textarea rows="4" <input type=textarea name=info></textarea></p>
		<p>Build : <br> <input type=file name=build></input></p>
		<p>Preview : <br> <input type=file name=image></input></p>
		<p><input type="Submit" name="Submit" value="Submit"></p>
	</form>
{{else}}
	{{> login}}
{{/if}}
